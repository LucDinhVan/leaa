{
  "name": "leaa-dashboard",
  "version": 2,
  "env": { "NODE_ENV": "production" },
  "builds": [{ "src": "**", "use": "@now/static" }],
  "routes": [
    { "src": "^/(scripts|styles|assets)/(.*)", "dest": "/admin/$1/$2" },
    { "src": "^/_env.js", "dest": "/admin/_env.js" },
    { "src": "^/(.*)", "dest": "/admin/index.html" }
  ]
}
