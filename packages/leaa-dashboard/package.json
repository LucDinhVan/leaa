    "clear": "cross-env rimraf _dist",
    "---------------------------------------------------------------------- PROD ----": "----",
    "build": "yarn clear && NODE_ENV=production cross-env webpack --config ./tools/webpack/webpack.config.js --progress --release",
    "---------------------------------------------------------------------- ALIAS ----": "----",
    "commit": "git-cz",
    "u": "yarn upgrade-interactive --latest"
